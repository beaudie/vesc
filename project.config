[access]
	inheritFrom = angle
[submit]
	action = cherry pick
[receive]
	requireChangeId = true
[label "Tryjob-Request"]
	function = NoOp
	value =  0
	value = +1 Approved for try server
	defaultValue = 0
	copyAllScoresOnTrivialRebase = true
	copyAllScoresIfNoCodeChange = true
[access "refs/heads/*"]
	label-Tryjob-Request = +0..+1 group angle-committers
	label-Tryjob-Request = +0..+1 group angle-contributors
	exclusiveGroupPermissions = label-Commit-Queue
  label-Commit-Queue = +0..+2 group angle-committers
[label "Commit-Queue"]
  function = NoBlock
  abbreviation = CQ
  value =  0 Not Ready
  value = +1 Dry Run
  value = +2 Ready
  # value = +3 Sheriff only: Ready, allow in throttled tree
  # This value is for eye-candy.
  # value = +4 Ignore this label
  defaultValue = 0
