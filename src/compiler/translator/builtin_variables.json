                        "type": {"basic": "Int", "precision": "High", "qualifier": "PrimitiveID"}
                    },
                    "gl_Layer":
                    {
                        "level": "ESSL3_1_BUILTINS",
                        "extension": "EXT_geometry_shader",
                        "type": {"basic": "Int", "precision": "High", "qualifier": "Layer"}
                    }
                }
            }
        }
